<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.8 -->
  <object class="GtkApplicationWindow" id="app_window">
    <property name="default-width">500</property>
    <property name="default-height">400</property>
    <property name="title" translatable="yes">Minecraft Server Manager</property>
    <property name="show-menubar">False</property>
    <!--child type="titlebar">
      <object class="GtkHeaderBar" id="header">
        <property name="title" translatable="yes">Minecraft Server Manager</property>
        <property name="subtitle" translatable="yes">Put IP here</property>
        <property name="visible">True</property>
        <property name="show-close-button">True</property>
        <child>
          <object class="GtkMenuButton" id="gears">
            <property name="visible">True</property>
            <property name="direction">none</property>
            <property name="use-popover">True</property>
            <style>
              <class name="image-button"/>
            </style>
          </object>
          <packing>
            <property name="pack-type">end</property>
          </packing>
        </child>
      </object>
    </child-->
    <child>
      <object class="GtkBox" id="content_box">
        <property name="visible">True</property>
        <property name="orientation">vertical</property>
        <property name="margin">2</property>
        <child>
          <object class="GtkBox" id="hbox">
            <property name="visible">True</property>
            <property name="spacing">3</property>
            <child>
              <object class="GtkBox" id="vbox">
                <property name="visible">True</property>
                <property name="orientation">vertical</property>
                <property name="vexpand">True</property>
                <property name="hexpand">False</property>
                <property name="width-request">150</property>
                <child>
                  <object class="GtkGrid" id="player_count_info">
                    <property name="visible">True</property>
                    <property name="margin">2</property>
                    <property name="row-spacing">2</property>
                    <property name="column-spacing">2</property>
                    <child>
                      <object class="GtkLabel" id="online_players_label">
                        <property name="visible">True</property>
                        <property name="label">Online: </property>
                        <property name="use-underline">True</property>
                        <property name="mnemonic-widget">online_players</property>
                        <property name="xalign">0</property>
                      </object>
                      <packing>
                        <property name="left-attach">0</property>
                        <property name="top-attach">0</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkLabel" id="online_players">
                        <property name="visible">True</property>
                        <property name="label">1</property>
                        <property name="xalign">1</property>
                        <property name="hexpand">True</property>
                      </object>
                      <packing>
                        <property name="left-attach">1</property>
                        <property name="top-attach">0</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkLabel" id="max_players_label">
                        <property name="visible">True</property>
                        <property name="label">Max: </property>
                        <property name="use-underline">True</property>
                        <property name="mnemonic-widget">max_players</property>
                        <property name="xalign">0</property>
                      </object>
                      <packing>
                        <property name="left-attach">0</property>
                        <property name="top-attach">1</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkLabel" id="max_players">
                        <property name="visible">True</property>
                        <property name="label">20</property>
                        <property name="can_focus">False</property>
                        <property name="xalign">1</property>
                        <property name="hexpand">True</property>
                      </object>
                      <packing>
                        <property name="left-attach">1</property>
                        <property name="top-attach">1</property>
                      </packing>
                    </child>
                  </object>
                </child>
                <!--child>
                  <object class="GtkSeparator">
                    <property name="visible">True</property>
                  </object>
                </child-->
                <child>
                  <object class="GtkScrolledWindow" id="sidebar">
                    <property name="visible">True</property>
                    <property name="hscrollbar-policy">never</property>
                    <property name="vscrollbar-policy">automatic</property>
                    <property name="vexpand">True</property>
                    <child>
                      <object class="GtkListBox" id="online_users">
                        <property name="visible">True</property>
                        <property name="selection-mode">none</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <!--child>
              <object class="GtkSeparator">
                <property name="visible">True</property>
                <property name="orientation">vertical</property>
              </object>
            </child-->
            <child>
              <object class="GtkBox" id="server">
                <property name="visible">True</property>
                <property name="orientation">vertical</property>
                <property name="vexpand">True</property>
                <property name="hexpand">True</property>
                <child>
                  <object class="GtkBox" id="server_info">
                    <property name="visible">True</property>
                    <child>
                      <object class="GtkImage" id="server_icon">
                        <property name="visible">True</property>
                        <property name="margin">2</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox" id="">
                        <property name="visible">True</property>
                        <property name="orientation">vertical</property>
                        <property name="margin">2</property>
                        <child>
                          <object class="GtkLabel" id="motd">
                            <property name="visible">True</property>
                            <property name="label">testing</property>
                            <property name="xalign">0</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkLabel" id="server_type">
                            <property name="visible">True</property>
                            <property name="label">Minecraft 1.15.2 SMP</property>
                            <property name="xalign">0</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkMenuButton" id="gears">
                        <property name="visible">True</property>
                        <property name="direction">none</property>
                        <property name="use-popover">True</property>
                        <style>
                          <class name="image-button"/>
                        </style>
                      </object>
                      <packing>
                        <property name="pack-type">end</property>
                      </packing>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkScrolledWindow" id="messages">
                    <property name="visible">True</property>
                    <property name="hscrollbar-policy">automatic</property>
                    <property name="vscrollbar-policy">automatic</property>
                    <property name="vexpand">True</property>
                    <child>
                      <object class="GtkListBox" id="messages_list">
                        <property name="visible">True</property>
                        <property name="selection-mode">none</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkEntry" id="command">
                    <property name="visible">True</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
