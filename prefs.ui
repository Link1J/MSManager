<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.8 -->
  <object class="GtkAdjustment" id="ports">
    <property name="upper">65565</property>
    <property name="value">25565</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkAdjustment" id="query_ports">
    <property name="upper">65565</property>
    <property name="value">25585</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkAdjustment" id="rcon_ports">
    <property name="upper">65565</property>
    <property name="value">25575</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkDialog" id="prefs_dialog">
    <property name="title" translatable="yes">Settings</property>
    <property name="resizable">False</property>
    <property name="modal">True</property>
    <property name="use-header-bar">1</property>
    <child internal-child="vbox">
      <object class="GtkBox" id="vbox">
        <child>
          <object class="GtkGrid" id="grid">
            <property name="visible">True</property>
            <property name="margin">6</property>
            <property name="row-spacing">12</property>
            <property name="column-spacing">6</property>
            <child>
              <object class="GtkLabel" id="ip_label">
                <property name="visible">True</property>
                <property name="label">IP:</property>
                <property name="use-underline">True</property>
                <property name="mnemonic-widget">ip</property>
                <property name="xalign">1</property>
              </object>
              <packing>
                <property name="left-attach">0</property>
                <property name="top-attach">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkEntry" id="ip">
                <property name="visible">True</property>
              </object>
              <packing>
                <property name="left-attach">1</property>
                <property name="top-attach">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="port_label">
                <property name="visible">True</property>
                <property name="label">Port:</property>
                <property name="use-underline">True</property>
                <property name="mnemonic-widget">port</property>
                <property name="xalign">1</property>
              </object>
              <packing>
                <property name="left-attach">0</property>
                <property name="top-attach">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkSpinButton" id="port">
                <property name="visible">True</property>
                <property name="value">25565</property>
                <property name="can_focus">True</property>
                <property name="adjustment">ports</property>
              </object>
              <packing>
                <property name="left-attach">1</property>
                <property name="top-attach">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="query_port_label">
                <property name="visible">True</property>
                <property name="label">Query Port:</property>
                <property name="use-underline">True</property>
                <property name="mnemonic-widget">query_port</property>
                <property name="xalign">1</property>
              </object>
              <packing>
                <property name="left-attach">0</property>
                <property name="top-attach">2</property>
              </packing>
            </child>
            <child>
              <object class="GtkSpinButton" id="query_port">
                <property name="visible">True</property>
                <property name="value">25585</property>
                <property name="can_focus">True</property>
                <property name="adjustment">query_ports</property>
                <property name="value">25585</property>
              </object>
              <packing>
                <property name="left-attach">1</property>
                <property name="top-attach">2</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="rcon_port_label">
                <property name="visible">True</property>
                <property name="label">RCON Port:</property>
                <property name="use-underline">True</property>
                <property name="mnemonic-widget">rcon_port</property>
                <property name="xalign">1</property>
              </object>
              <packing>
                <property name="left-attach">0</property>
                <property name="top-attach">3</property>
              </packing>
            </child>
            <child>
              <object class="GtkSpinButton" id="rcon_port">
                <property name="visible">True</property>
                <property name="value">25575</property>
                <property name="can_focus">True</property>
                <property name="adjustment">rcon_ports</property>
              </object>
              <packing>
                <property name="left-attach">1</property>
                <property name="top-attach">3</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="rcon_password_label">
                <property name="visible">True</property>
                <property name="label">RCON Password:</property>
                <property name="use-underline">True</property>
                <property name="mnemonic-widget">rcon_password</property>
                <property name="xalign">1</property>
              </object>
              <packing>
                <property name="left-attach">0</property>
                <property name="top-attach">4</property>
              </packing>
            </child>
            <child>
              <object class="GtkEntry" id="rcon_password">
                <property name="visible">True</property>
                <property name="input_purpose">password</property>
              </object>
              <packing>
                <property name="left-attach">1</property>
                <property name="top-attach">4</property>
              </packing>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
